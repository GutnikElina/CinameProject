<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="250.0" prefWidth="426.0" spacing="15" style="-fx-background-color: #8d85a6;" xmlns="http://javafx.com/javafx/17.0.13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.SessionActions.DeleteSession">
    <padding>
        <Insets bottom="20.0" left="70.0" right="70.0" top="20.0" />
    </padding>
    <Label text="Удаление сеанса">
        <font>
            <Font name="Segoe UI Semibold" size="25.0" />
        </font></Label>
    <TextField fx:id="sessionIdField" prefHeight="36.0" prefWidth="200.0" promptText="Введите ID сеанса">
        <VBox.margin>
            <Insets left="50.0" right="50.0" />
        </VBox.margin></TextField>
    <Button onAction="#deleteSessionAction" prefHeight="33.0" prefWidth="189.0" styleClass="button" text="Удалить фильм" textFill="WHITE">
        <font>
            <Font name="Segoe UI Semibold" size="13.0" />
        </font></Button>
    <Button fx:id="backButton" onAction="#handleBackButton" prefHeight="32.0" prefWidth="189.0" styleClass="manage" text="Назад" textFill="WHITE" />
</VBox>
